{
    "presets": [
        [
          "@babel/preset-env",
          {
              "targets": {
                  "node": "current",
                  "browsers": ["last 1 versions", "ie <= 11"]
              },
              "useBuiltIns": "usage",
              "corejs": 3
          }
        ],
        "@babel/preset-typescript",
        "@babel/preset-react"
    ],
    "plugins": [
        "@babel/plugin-transform-runtime",
        "@babel/proposal-class-properties",
        "@babel/plugin-proposal-optional-chaining",
        "@babel/proposal-object-rest-spread"
    ]
}